@base <https://sparql.uniprot.org/.well-known/sparql-examples/M74.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M74_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "Taxon, Scientific name and super Class"@en;
  sh:prefixes _:genid-9505b3c793ad4fdeb36bc733b4b55a462-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?taxon ?superTaxon ?name ?rank
WHERE {
        ?taxon rdf:type up:Taxon.
        ?taxon up:scientificName ?name.
        ?taxon rdfs:subClassOf ?superTaxon.
        ?taxon up:rank ?rank.
}""";
  schema:keywords "taxonomy";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-taxon>, <#port-name>, <#port-superTaxon>, <#port-rank> .

<#port-name>
  a smodule:ModulePort;
  rdfs:domain xsd:string ;
  smodule:name "name" .

<#port-rank>
  a smodule:ModulePort;
  rdfs:domain up:Rank ;
  smodule:name "rank" .

<#port-taxon>
  a smodule:ModulePort;
  rdfs:domain up:Taxon;
  smodule:name "taxon" .

<#port-superTaxon>
  a smodule:ModulePort;
  rdfs:domain up:Taxon ;
  smodule:name "superTaxon" .
