@base <https://sparql.uniprot.org/.well-known/sparql-examples/M63.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M63_module a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Bgee IRI conversion"@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX up: <http://purl.uniprot.org/core/>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?ensemblGeneNoVer ?ensemblGene
WHERE {
	BIND( IRI(REPLACE(STR(?ensemblGene), "\\\\.[0-9]+$", "")) AS ?ensemblGeneNoVer )   # strip version from Ensembl ID
}""" ;
    schema:keywords "Bgee" ;
    schema:target <https://sparql.uniprot.org/sparql/> ;
    smodule:output <#port-ensemblGene>, <#port-ensemblGeneNoVer>.
    
<#port-ensemblGene> 
	a smodule:ModulePort ; 
	smodule:name "ensemblGene" ;
	rdfs:domain up:Gene.

<#port-ensemblGeneNoVer> 
	a smodule:ModulePort ;
	smodule:name "ensemblGeneNoVer" ;
	rdfs:domain xsd:string.
