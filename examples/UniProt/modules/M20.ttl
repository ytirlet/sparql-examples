@base <https://sparql.uniprot.org/.well-known/sparql-examples/M20.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M20_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "Annotation attribution"@en;
  sh:prefixes _:genid-957ebfe3624441a187b8b96f0207b3b22-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?annotation ?object ?attribution ?source ?sourceType
WHERE {
        ?object rdf:object ?annotation.
        ?object up:attribution ?attribution.
        ?attribution up:source ?source.
        ?source rdf:type ?sourceType.
        ?attribution up:evidence ?evidence.
}""";
  schema:keywords "annotation", "evidence tag", "PubMed";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-annotation>, <#port-object>, <#port-attribution>, <#port-sourceType>, <#port-source>, <#port-evidence> .

<#port-annotation>
  a smodule:ModulePort;
  rdfs:domain up:Annotation ;
  smodule:name "annotation" .

<#port-object>
  a smodule:ModulePort;
  smodule:name "object" .

<#port-attribution>
  a smodule:ModulePort;
  smodule:name "attribution" .

<#port-sourceType>
  a smodule:ModulePort;
  smodule:name "sourceType" .

<#port-source>
  a smodule:ModulePort;
  smodule:name "source" .

<#port-evidence>
  a smodule:ModulePort;
  smodule:name "evidence".
