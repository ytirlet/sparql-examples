@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:M6_module a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Entry, type and associated organism and super Class of the organism"@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX up: <http://purl.uniprot.org/core/>
SELECT ?protein ?organism ?taxon
WHERE {
	?protein rdf:type up:Protein.
	?protein up:organism ?organism.
	?organism rdfs:subClassOf ?taxon.
}""" ;
    schema:keywords "protein organism", "protein taxon" ;
    schema:target <https://sparql.uniprot.org/sparql/> .
