@base <https://sparql.uniprot.org/.well-known/sparql-examples/M23.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M23_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "Protein GO classification"@en;
  sh:prefixes _:genid-68eb1b9717dd4d8d9d371c401521d0ae2-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?protein ?goTerm ?superGOTerm
WHERE {
        ?protein up:classifiedWith ?goTerm.
        ?protein rdf:type up:Protein.
        ?goTerm rdfs:subClassOf ?superGOTerm.
        ?goTerm rdfs:label ?label.
}""";
  schema:keywords "protein classification", "GO annotation";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-protein>, <#port-goTerm>, <#port-superGOTerm>, <#port-label>.

<#port-protein>
  a smodule:ModulePort;
  smodule:name "protein" ;
  rdfs:domain up:Protein .

<#port-goTerm>
  a smodule:ModulePort;
  smodule:name "goTerm" .

<#port-superGOTerm>
  a smodule:ModulePort;
  smodule:name "superGO" .

<#port-label>
  a smodule:ModulePort;
  rdfs:domain xsd:string ;
  smodule:name "label".
