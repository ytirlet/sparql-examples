@base <https://sparql.uniprot.org/.well-known/sparql-examples/M17.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M17_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "Entry informations"@en;
  sh:prefixes _:genid-a998ebdc3a924a4c8cb1ae25054b75a72-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?protein ?creationDate ?modificationDate ?reviewed ?version
WHERE {
        ?protein rdf:type up:Protein ;
                up:created ?creationDate ;
                up:modified ?modificationDate ;
                up:reviewed ?reviewed ;
                up:version ?version .
}""";
  schema:keywords "date", "entry history";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-protein>, <#port-creationDate>, <#port-modificationDate>, <#port-reviewed>, <#port-version>.

<#port-creationDate>
  a smodule:ModulePort;
  rdfs:domain xsd:date ;
  smodule:name "creationDate" .

<#port-protein>
  a smodule:ModulePort;
  rdfs:domain up:Protein ;
  smodule:name "protein" .

<#port-modificationDate>
  a smodule:ModulePort;
  rdfs:domain xsd:date ;
  smodule:name "modificationDate" .

<#port-reviewed>
  a smodule:ModulePort;
  rdfs:domain xsd:boolean ;
  smodule:name "reviewed" .

<#port-version>
  a smodule:ModulePort;
  rdfs:domain xsd:int ;
  smodule:name "version" .
