@base <https://sparql.uniprot.org/.well-known/sparql-examples/M52_subcellular_location> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix uniprotkb: <http://purl.uniprot.org/uniprot/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<> a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "protein and location"@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX up: <http://purl.uniprot.org/core/>
    PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?protein ?annotation ?location_inside_cell ?cellComponent
WHERE {
	?protein up:annotation ?annotation.
	?protein rdf:type up:Protein.
	?annotation up:locatedIn/up:cellularComponent ?cellComponent.
	?cellComponent skos:prefLabel ?location_inside_cell.
}""" ;
    schema:keywords "protein annotation", "cellular component" ;
    schema:target <https://sparql.uniprot.org/sparql/> ;
    smodule:output <#port-protein>, <#port-annotation>, <#port-location_inside_cell>, <#port-cellComponent>.
    
<#port-protein> 
	a smodule:ModulePort ;
	smodule:name "protein" ;
	rdfs:domain up:Protein ;
	smodule:example uniprotkb:P01308, uniprotkb:A0A002, uniprotkb:A0A008ARC3.
	
<#port-annotation> 
	a smodule:ModulePort ;
	smodule:name "annotation" ;
	rdfs:domain up:Subcellular_Location_Annotation.

<#port-location_inside_cell> 
	a smodule:ModulePort ;
	smodule:name "location_inside_cell" ;
	rdfs:domain xsd:string.

<#port-cellComponent> 
	a smodule:ModulePort ; 
	smodule:name "cellComponent".
