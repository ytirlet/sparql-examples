@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:M16_module a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "protein, annotation, type and range of annotation"@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX up: <http://purl.uniprot.org/core/>
SELECT ?protein ?annotation ?annType ?range
WHERE {
	?protein rdf:type up:Protein.
	?protein up:annotation ?annotation.
	?annotation rdf:type ?annType.
	?annotation up:range ?range.
}""" ;
    schema:keywords "protein annotation", "annotation type", "annotation range" ;
    schema:target <https://sparql.uniprot.org/sparql/> .
