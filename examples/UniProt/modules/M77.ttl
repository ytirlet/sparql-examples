@base <https://sparql.uniprot.org/.well-known/sparql-examples/M77.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M77_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "Protein to proteome"@en;
  sh:prefixes _:genid-a5a7c406b5704bba8032a9ff320543ac2-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?protein ?proteome
WHERE {
        ?protein rdf:type up:Protein.
        ?protein up:proteome ?proteome .
}""";
  schema:keywords "proteome";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-protein>, <#port-proteome>.

<#port-protein>
  a smodule:ModulePort;
  rdfs:domain up:Protein;
  smodule:name "protein" .

<#port-proteome>
  a smodule:ModulePort;
  rdfs:domain up:Proteome_Component ;
  smodule:name "proteome" .
 
