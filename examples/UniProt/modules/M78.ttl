@base <https://sparql.uniprot.org/.well-known/sparql-examples/M78.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M78_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "Any Name"@en;
  sh:prefixes _:genid-2d0dc5b16a874d7192dc80da3a65f7de2-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?entry ?anyName
WHERE {
        { ?entry up:recommendedName ?anyName.
        }
        UNION {
        ?entry up:recommendedName ?recName.
        ?recName up:fullName ?anyName.
        }
        UNION {
        ?entry up:recommendedName ?recName.
        ?recName up:shortName ?anyName.
        }
        UNION {
        ?entry up:alternativeName ?anyName.
        }
}""";
  schema:keywords "name";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-entry>, <#port-Name>.

<#port-anyName>
  a smodule:ModulePort;
  smodule:name "anyName" .

<#port-entry>
  a smodule:ModulePort;
  rdfs:domain [
		a owl:Class ;
		smodule:option (up:Protein up:Part)
		]; 
  smodule:name "entry" .

