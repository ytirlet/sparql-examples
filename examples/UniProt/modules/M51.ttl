@base <https://sparql.uniprot.org/.well-known/sparql-examples/M51.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:M51_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "protein and associated disease"@en;
  sh:prefixes _:genid-a9e564c4f244461280d810ae10be072c2-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?protein ?annotation ?disease
WHERE {
        ?protein up:annotation ?annotation.
        ?protein rdf:type up:Protein.
        ?annotation rdf:type up:Disease_Annotation.
        ?annotation up:disease ?disease.
        ?disease rdf:type up:Disease.
}""";
  schema:keywords "protein annotation", "disease";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-protein>, <#port-annotation>, <#port-disease> .
  

<#port-annotation>
  a smodule:ModulePort;
  smodule:name "annotation" ;
  rdfs:domain up:Disease_Annotation.

<#port-disease>
  a smodule:ModulePort;
  smodule:name "disease" .

<#port-protein>
  a smodule:ModulePort;
  rdfs:domain up:Protein;
  smodule:name "protein" .
