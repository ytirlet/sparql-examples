@base <https://sparql.uniprot.org/.well-known/sparql-examples/M32.ttl> .
@prefix ex: <https://sparql.uniprot.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix smodule: <https://sparql_module.biographs.org/> .
@prefix up: <http://purl.uniprot.org/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ex:M32_module a sh:SPARQLExecutable, sh:SPARQLSelectExecutable;
  rdfs:comment "Entry sequence"@en;
  sh:prefixes _:genid-f141d7ece9e04ea5a0f0105edefab5512-sparql_examples_prefixes;
  sh:select """PREFIX up: <http://purl.uniprot.org/core/>
  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?entry ?sequenceType ?sequence ?value ?entryType
WHERE {
        ?entry rdf:type ?entryType.
        ?entry up:sequence ?sequence.
        ?sequence rdf:type ?sequenceType.
        ?sequence rdf:value ?value.
}""";
  schema:keywords "sequence";
  schema:target <https://sparql.uniprot.org/sparql/> ;
  smodule:output <#port-entry>, <#port-entryType>, <#port-sequence> , <#port-sequenceType>, <#port-value>.

<#port-entry>
  a smodule:ModulePort;
  rdfs:domain [
		a owl:Class ;
		smodule:option (up:Annotation up:Ressource up:Protein)
		]; 
  smodule:name "entry" .

<#port-sequence>
  a smodule:ModulePort;
  smodule:name "sequence" .

<#port-entryType>
  a smodule:ModulePort;
  smodule:option (up:Annotation up:Ressource up:Protein) ;
  smodule:name "entryType" .
  
<#port-sequenceType>
  a smodule:ModulePort;
  smodule:name "sequenceType" .
  
<#port-value>
  a smodule:ModulePort;
  rdfs:domain xsd:string;
  smodule:name "value" .
